{
    "@context": "dtmi:dtdl:context;2",
    "@id": "dtmi:io:tributech:device:base;1",
    "@type": "Interface",
    "displayName": "Device",
    "contents": [
        {
            "@type": "Property",
            "name": "DeviceId",
            "schema": "string",
            "writable": false,
            "displayName": "Device Id",
            "comment": "Represents the unique ID of the Tributech-device."
        },
        {
            "@type": "Property",
            "name": "PublicKey",
            "schema": "string",
            "writable": false,
            "displayName": "Publik key",
            "comment": "Represents the current public-key of the device which matches with the current key-pair generated inside the Optiga Trust M chip."
        },
        {
            "@type": "Property",
            "name": "AgentConfig",
            "schema": {
                "@type": "Map",
                "mapKey": {
                    "name": "key",
                    "schema": "string"
                },
                "mapValue": {
                    "name": "value",
                    "schema": "string"
                }
            }
        },
        {
            "@type": "Property",
            "name": "KeyStorageType",
            "schema": {
                "@type": "Enum",
                "valueSchema": "string",
                "enumValues": [
                    {
                        "name": "None",
                        "displayName": "None",
                        "enumValue": "NONE"
                    },
                    {
                        "name": "Tpm20",
                        "displayName": "TPM 20",
                        "enumValue": "TPM_2_0"
                    },
                    {
                        "name": "OptigaTrustM",
                        "displayName": "Optiga Trust M",
                        "enumValue": "OPTIGA_TRUST_M"
                    }
                ]
            },
            "writable": false,
            "displayName": "Key Storage Type",
            "description": ""
        },
        {
            "@type": "Property",
            "name": "ProofKind",
            "schema": {
                "@type": "Enum",
                "valueSchema": "string",
                "enumValues": [
                    {
                        "name": "SHA256_RSA2048_PSS",
                        "displayName": "SHA256_RSA2048_PSS",
                        "enumValue": "SHA256_RSA2048_PSS"
                    },
                    {
                        "name": "SHA256_RSA2048_PKCS1",
                        "displayName": "SHA256_RSA2048_PKCS1",
                        "enumValue": "SHA256_RSA2048_PKCS1"
                    }
                ]
            },
            "writable": false,
            "displayName": "Proof Kind",
            "description": ""
        },
        {
            "@type": "Relationship",
            "name": "Sources",
            "minMultiplicity": 0,
            "maxMultiplicity": 10,
            "target": "dtmi:io:tributech:source:base;1",
            "displayName": "Sources",
            "description": "Contains all sources of the given agent."
        },
        {
            "@type": "Relationship",
            "name": "Annotations",
            "minMultiplicity": 0,
            "maxMultiplicity": 1,
            "target": "dtmi:io:tributech:annotations:base;1",
            "displayName": "Annotations",
            "description": "Annotations"
        },
        {
            "@type": "Relationship",
            "name": "Sinks",
            "minMultiplicity": 0,
            "maxMultiplicity": 1,
            "target": "dtmi:io:tributech:sink:base;1",
            "displayName": "Sinks",
            "description": "Sinks"
        }
    ]
}