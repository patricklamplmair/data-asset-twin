{
  "@context": "dtmi:dtdl:context;2",
  "@id": "dtmi:io:tributech:ssm;1",
  "@type": "Interface",
  "displayName": "SSM",
  "contents": [
    {
      "@type": "Property",
      "name": "DeviceId",
      "schema": "string",
      "writable": false,
      "displayName": "Device Id",
      "description": "Represents the unique ID of the Tributech-SSM-device."
    },
    {
      "@type": "Property",
      "name": "Id",
      "schema": "string",
      "writable": false,
      "displayName": "Id",
      "description": "Identifier of the data-set."
    },
    {
      "@type": "Property",
      "name": "PublicKey",
      "schema": "string",
      "writable": false,
      "displayName": "Public key",
      "description": "Represents the current public-key of the device which matches with the current key-pair generated inside the Optiga Trust M chip."
    },
    {
      "@type": "Property",
      "name": "SoftwareVersion",
      "schema": "string",
      "writable": true,
      "displayName": "Software Version",
      "description": "Represents the current running software version."
    },
    {
      "@type": "Property",
      "name": "HardwareVersion",
      "schema": "string",
      "writable": true,
      "displayName": "Hardware Version",
      "description": "Represents the current running hardware version."
    },
    {
      "@type": "Property",
      "name": "KeyStorageType",
      "schema": {
        "@type": "Enum",
        "valueSchema": "string",
        "enumValues": [
          {
            "name": "None",
            "displayName": "None",
            "enumValue": "NONE"
          },
          {
            "name": "Tpm20",
            "displayName": "TPM 20",
            "enumValue": "TPM_2_0"
          },
          {
            "name": "OptigaTrustM",
            "displayName": "Optiga Trust M",
            "enumValue": "OPTIGA_TRUST_M"
          }
        ]
      },
      "writable": false,
      "displayName": "Key Storage Type",
      "description": ""
    },
    {
      "@type": "Property",
      "name": "ProofKind",
      "schema": {
        "@type": "Enum",
        "valueSchema": "string",
        "enumValues": [
          {
            "name": "SHA256_RSA2048_PSS",
            "displayName": "SHA256_RSA2048_PSS",
            "enumValue": "SHA256_RSA2048_PSS"
          },
          {
            "name": "SHA256_RSA2048_PKCS1",
            "displayName": "SHA256_RSA2048_PKCS1",
            "enumValue": "SHA256_RSA2048_PKCS1"
          }
        ]
      },
      "writable": false,
      "displayName": "Proof Kind",
      "description": ""
    },
    {
      "@type": "Property",
      "name": "SubmitValues",
      "schema": "boolean",
      "writable": true,
      "displayName": "Submit telemetry values",
      "description": "Specifies it the submission of telemetry-data is activated or not."
    },
    {
      "@type": "Property",
      "name": "SubmitProofs",
      "schema": "boolean",
      "writable": true,
      "displayName": "Submit proofs of telemetry-values.",
      "description": "Specifies it the submission of integrity- and origin-verification data is activated or not."
    },
    {
      "@type": "Property",
      "name": "AgentConfig",
      "schema": {
        "@type": "Map",
        "mapKey": {
          "name": "key",
          "schema": "string"
        },
        "mapValue": {
          "name": "value",
          "schema": "string"
        }
      }
    },
    {
      "@type": "Relationship",
      "name": "Sources",
      "minMultiplicity": 0,
      "maxMultiplicity": 10,
      "target": "dtmi:io:tributech:generic:source;1",
      "displayName": "Sources",
      "description": "Contains all sources of the given agent."
    },
    {
      "@type": "Relationship",
      "name": "MlAnnotation",
      "minMultiplicity": 0,
      "maxMultiplicity": 1,
      "target": "dtmi:io:tributech:annotations:ml;1",
      "displayName": "Machine learning annotation",
      "description": "Machine learning annotation."
    }
  ]
}